@all:  [A] $[N] {
	echo >&2 "N='$N'"
	ls -l list.*
	[ -e list."$N" ]       || exit 1
	[ -e list."$((N+1))" ] && exit 1
	exit 0
}

N = {41}

>A:  N
{
	for i in $(seq 1 $(cat N)) ; do
		printf '%s' '['
	done
	printf list.0
	for i in $(seq 1 $(cat N)) ; do
		printf '%s' ']'
	done
	echo
}

>list.$n: 
{
	echo list."$((n + 1))"
}
